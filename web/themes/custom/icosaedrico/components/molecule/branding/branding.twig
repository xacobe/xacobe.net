<div {{ attributes.addClass }}>
  {{ title_prefix }}
  {% if label %}
    <h2 {{ title_attributes }}>{{ label }}</h2>
  {% endif %}
  {{ title_suffix }}

  {% block content %}
    {% if site_logo %}
      <a href="{{ path('<front>') }}" rel="home"><img src="{{ site_logo }}" alt="{{ 'Home'|t }}" fetchpriority="high" /></a>
    {% endif %}
    {% if site_name %}
      <a href="{{ path('<front>') }}" rel="home">{{ site_name }}</a>
    {% endif %}
    {{ site_slogan }}
  {% endblock %}
</div>
